<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_looked"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.admin.academicheadlines.LookedActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/buju3"
        android:background="#ffffff">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:text="浏览历史"
            android:gravity="center"
            android:id="@+id/TextView2"
            android:textSize="15sp"
            android:textColor="#000000"/>

        <Button
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:text="清空"
            android:background="#ffffff"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/button2" />
    </RelativeLayout>

    <Button
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/papertitle3"
        android:text="ICA with Reconstruction Cost for Efficient Overcomplete Feature Learning"
        android:gravity=""
        android:background="#6699"
        android:layout_below="@id/buju3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="19dp" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/papertitle4"
        android:text="Sparse Bayesian Learning and the Relevance Vector Machine"
        android:gravity=""
        android:background="#6699"
        android:layout_below="@+id/papertitle3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="15dp" />
</RelativeLayout>
